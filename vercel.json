{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "build": {
        "env": {
            "DATABASE_URL": "@your-database-url"
        },
        "commands": {
            "prisma_generate": "prisma generate"
        }
    }
}